{
    "name": "evheniy/jquery-bundle",
    "description": "The JqueryBundle adds the ability to add latest jQuery to your application.",
    "keywords": ["jQuery", "Symfony", "latest"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "evheniy",
            "homepage": "https://github.com/evheniy"
        }
    ],
    "require": {
        "php":             ">=5.5.9",
        "symfony/symfony": ">=3.0",
        "symfony/assetic-bundle": "~2.5-dev"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Evheniy\\JqueryBundle": ""
        }
    },
    "target-dir" : "Evheniy/JqueryBundle"
}
